<div class="row-fluid">
    <div class="span12">
        <div class="widget box light-grey">
            <div class="widget-title">
                <h4><i class="icon-globe"></i><hd lang_l="l_head">用户管理 @ViewBag.Rand</hd></h4>
                <div class="actions">
                    <a class="btn btn-primary" href="javascript:void(0)" onclick="User.Add()"><i class="icon-pencil"></i>&nbsp;<hd lang_l="lc_add">新增</hd></a>
                    <a class="btn btn-danger" href="javascript:void(0)" onclick="User.BatchDel()"><i class="icon-trash"></i>&nbsp;<hd lang_l="lc_delete">删除</hd></a>
                </div>
            </div>
            <div class="widget-body">
                <div class="row-fluid">
                    <form class="form-horizontal" action="javascript:void(0)">
                        <div class="control-group">
                            <div class="controls controls-cus">
                                <input id="txtSearch" type="text" class="input-xlarge" lang_p="p_userCodeorName" placeholder="请输入用户名或者编号" />
                                <a id="gritter-remove-all" class="btn btn-info" href="javascript:void(0)" onclick="User.PageClick(1)"><i class="icon-search"></i>&nbsp;<hd lang_l="lc_search">搜索</hd></a>
                                <div class="btn-group">
                                    <button id="btnHSearch" data-toggle="dropdown" class="btn btn-primary dropdown-toggle" lang_l="l_advancedSearch">高级搜索 <span class="caret"></span></button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div id="divHSearch" class="row-fluid hide">
                    <div class="widget">
                        <div class="widget-body form">
                            <form class="horizontal-form" action="javascript:void(0)">
                                <div class="row-fluid">
                                    <div class="span2">
                                        <div class="control-group">
                                            <label for="firstName" class="control-label" lang_l="l_employeeNo">员工编号</label>
                                            <div class="controls">
                                                <input id="txtUserCode" name="txtUserCode" type="text" class="span12" />
                                                <span class="help-block"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span2">
                                        <div class="control-group">
                                            <label for="firstName" class="control-label" lang_l="l_employeeName">员工姓名</label>
                                            <div class="controls">
                                                <input id="txtUserName" name="txtUserName" type="text" class="span12" />
                                                <span class="help-block"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span2">
                                        <div class="control-group">
                                            <label for="firstName" class="control-label" lang_l="l_department">部门</label>
                                            <div class="controls">
                                                <select id="ddlDepart" name="ddlDepart" class="input-medium">
                                                    @Html.Raw(ViewBag.DepartList)
                                                </select>
                                                <span class="help-block"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span2">
                                        <div class="control-group">
                                            <label for="firstName" class="control-label" lang_l="l_role">角色</label>
                                            <div class="controls">
                                                <select id="ddlRole" name="ddlRole" class="input-medium">
                                                    @Html.Raw(ViewBag.RoleList)
                                                </select>&nbsp;&nbsp;
                                                <span class="help-block"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span2">
                                        <div class="control-group">
                                            <label for="firstName" class="control-label">&nbsp;&nbsp;</label>
                                            <a class="btn btn-info" href="javascript:void(0)" onclick="User.PageClick(1)"><i class="icon-search"></i>&nbsp;<hd lang_l="lc_search">搜索</hd></a>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
