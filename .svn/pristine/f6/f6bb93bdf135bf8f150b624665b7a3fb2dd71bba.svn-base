/* CSS Document */
@charset "utf-8";

/*头部菜单*/
.top{ padding:0px 0px;}
.top img{width:100%;}

/*车源列表*/
.carlist{width:100%;}
.carlist_s
{
    padding: 0px 0px;
    text-align: center;
    background-color: #DBDBDB;
    border-bottom:1px solid #999;
    padding:5px 0px;
}
.carlist_s table{ border:0px; padding:0px; border-collapse:collapse; border-spacing:0; width:100%;}
.carlist_s table tr td{ text-align:center;}
.carlist_s select{ border:0px; width:65px; background:url(images/arrdown.png) no-repeat transparent; background-position:right center; background-size:20% auto; margin:0px auto; font-size:1.5rem;appearance:none;   -moz-appearance:none;   -webkit-appearance:none;}
.carlist_s select::-ms-expand { display: none; }
.carlist1 ul li{ padding:10px 5px; border-bottom:1px #999 solid;}
.carlist1 table{ border:0px; padding:0px; border-collapse:collapse; border-spacing:0; width:100%;}
.carlist1 table tr th{width:38%; padding-right:2%; }
.carlist1 table tr th img{width:100%;}
.carlist1 table tr td{ line-height:2.5rem;}
.carlist1 table tr td h1{font-size:1.6rem; font-weight:bold; display:inline;}
.carlist1 table tr td img{ width:100%; padding:0px; margin:0px;}
.carlist1 table tr td span{color: #999999; font-size: 1.4rem;}
.carlist1 table tr td label{ color:#ff0000; font-size:1.8rem;}

/*更多*/
.more{padding:10px 5px; border-bottom:1px #999 solid; text-align:center;}
.more img{width:40px;}
/*.more input{ display:block; text-align:center; width:70%; padding:5px 0px; border:1px solid #CCC; background-color:#F4F4F9; margin:0px auto;}*/
.more input{ display:block; text-align:center; width:70%; padding:5px 0px; border:0px solid #CCC; background-color:transparent; margin:0px auto;}

/*车源详情*/
.carinfo{width:-moz-calc(100% - 10px);
	width:-webkit-calc(100% - 10px);width:calc(100% - 10px); padding:10px 5px;}
.carinfo1{border-bottom:1px #999 solid;}
.carinfo2{ padding:10px 0px;}
.carinfo2 table{width:100%;}
.carinfo2 table tr th{width:60%; font-size:1.2rem; color:#595656;}
.carinfo2 table tr th h1{ color:#da241b; font-size:3rem;}
.carinfo2 table tr th h2{ color:#1472a9;font-size:1.5rem; display:inline;}
.carinfo2 table tr td{width:40%;}
.carinfo2 table tr td img{width:80%; height:auto;}
.carinfo2 table tr td h3{ display:block; text-align:center; width:95%;}
.carinfo2 table tr td label{ display:block; width:-moz-calc(100% - 33px);
	width:-webkit-calc(100% - 33px);width:calc(100% - 33px); padding:12px 0px 12px 33px;  text-align:left; vertical-align:middle; background:url(images/tel2.png) no-repeat; background-size:100% auto;}
.carinfo2 table tr td label a:link,.carinfo2 table tr td label a:hover,.carinfo2 table tr td label a:visited{font-weight:bold; color:#fff; font-size:1.3rem;}
.carinfo3{ padding:10px 0px;}
.carinfo3 table{width:100%; font-size:1.3rem;}
.carinfo3 table tr th{ text-align:right; padding:3px;}
.carinfo3 table tr td{ text-align:left; padding:3px;}
.carinfo3t{ background:url(images/index_34.jpg) no-repeat #CFDAF1;background-size:auto 60%; background-position:10px 7px; font-size:1.5rem; color:#595656;  padding:5px 25px;}
.carinfo4s{ padding:5px 0px;}
.carinfo4s ul li{ float:left; background-color:#ffebec; margin-left:10px; margin-bottom:5px; color:#ee877d; border:1px solid #ee877d; padding:2px 5px; -moz-border-radius:3px; -webkit-border-radius:3px; border-radius:3px;}
.carinfo4s img{width:100%;}
.carinfo4s1{ padding:10px 20px; font-size:1.5rem; line-height:2.2rem;}
.carinfo4{ padding:10px 25px;}
.carinfo4 ul li{ background:url(images/box1.jpg) no-repeat 0px 5px; padding:5px 5px 5px 20px;width:-moz-calc(50% - 25px);
	width:-webkit-calc(50% - 25px);width:calc(50% - 25px); float:left;}
.carinfo4 ul .selected{ background:url(images/box2.jpg) no-repeat 0px 5px; padding:5px 5px 5px 20px;width:-moz-calc(50% - 25px);
	width:-webkit-calc(50% - 25px);width:calc(50% - 25px); float:left;}
.carinfo5{ padding:10px 25px;}
.carinfo5 ul li{background:url(images/box1.jpg) no-repeat 0px 5px; padding:5px 5px 5px 20px;width:-moz-calc(50% - 25px);
	width:-webkit-calc(50% - 25px);width:calc(50% - 25px); float:left;}
.carinfo5 ul .selected{background:url(images/box2.jpg) no-repeat 0px 5px; padding:5px 5px 5px 20px;width:-moz-calc(50% - 25px);
	width:-webkit-calc(50% - 25px);width:calc(50% - 25px); float:left;}

/*业务列表*/
.pglist{border-top:1px #999 solid;}
.pglist ul li{ padding:10px 5px;border-bottom:1px #999 solid;}
.pglist ul li table{width:100%;}
.pglist ul li table tr th{width:40%; text-align:center;}
.pglist ul li table tr td{width:60%; text-align:left; font-size:1.3rem; line-height:1.7rem;}
.pglist ul li table tr td img{width:5%;}

/*关于我们*/
.about{ padding:10px;}
.about1{ font-size:2.3rem; padding:10px;border-bottom:1px #999 solid; text-align:center;}
.about2{ padding:10px;}
.about3{ font-size:2.3rem; padding:10px;border-bottom:1px #999 solid;border-top:1px #999 solid; text-align:center;}
.about4{ padding:10px;}
.about4 table tr th{width:20%;}
.about4 table tr td{width:80%;}
.about5{ font-size:2.3rem; padding:10px;border-bottom:1px #999 solid;border-top:1px #999 solid; text-align:center;}
.about6{ text-align:center; padding:10px;}

/*漂浮*/
.piao{ position:fixed; bottom:50px; right:5px;background-color: #337bff;color: white;padding: 2px 5px;}
.piao1{ position:absolute; z-index:99; margin-left:-278px; display:none;}
.piao1 ul li{ background-color:#12529C; color:#fff; display:block; float:left; width:70px; padding:13px 10px 12px 10px; text-align:center; margin-left:2px;}
.piao1 ul li a:link,.piao1 ul li a:visited,.piao1 ul li a:hover{ color:#fff; font-weight:bold;}

/*空行*/
.bgLine{width:-moz-calc(100% - 20px);
	width:-webkit-calc(100% - 20px);width:calc(100% - 20px); padding:15px 10px;}

/*底部漂浮*/
.bottom{width:-moz-calc(100% - 20px); border-top:solid 1px #999;
	width:-webkit-calc(100% - 20px);width:calc(100% - 20px); padding:5px 10px; bottom:0;  position:fixed; z-index:9;background-color:#f7f7f7; text-align:center; clear:both;}
.bottom table{width:100%; border:0px; border-collapse:collapse; border-spacing:0; padding:0px; margin:0px;}
.bottom table tr th{width:46%; text-align:center;}
.bottom table tr th a:link,.bottom table tr th a:hover,.bottom table tr th a:visited{font-size:1.6rem;}
.bottom table tr th img{width:20%; vertical-align:middle; margin-right:7px;}
.bottom table tr td{width:18%; text-align:center; line-height:1.2rem;}
.bottom table tr td a:link,.bottom table tr td a:hover,.bottom table tr td a:visited{ font-size:1.2rem;}
.bottom table tr td img{width:40%;}

/*分页*/
.pagebar{ padding-top:10px; padding-bottom:10px; text-align:center;}
.pagebar img{ vertical-align:middle;}
.pagebar a,.pagebar a:link,.pagebar a:visited,.pagebar a:hover{ padding:2px 5px 2px 5px; margin-left:5px; color:#0067c5;}
.pagebar .current{ padding:2px 5px 2px 5px; margin-left:5px; font-weight:bold;}
.pagebar .current:link,.pagebar .current:hover,.pagebar .current:visited{ color:#ffffff; font-weight:bold;}

/*搜索*/
.carsearch{ width:-moz-calc(100% - 40px);
	width:-webkit-calc(100% - 40px);width:calc(100% - 40px);padding:5px 20px; text-align:center;}
.carsearch1{ width:-moz-calc(100% - 20px);
	width:-webkit-calc(100% - 20px);width:calc(100% - 20px); padding:7px 10px; -moz-border-radius:25px; -webkit-border-radius:25px; border-radius:25px; border:1px solid #050505;}
.carsearch2{ width:8%; height:auto; position:absolute; right:20px; z-index:1; margin-top:-32px;}